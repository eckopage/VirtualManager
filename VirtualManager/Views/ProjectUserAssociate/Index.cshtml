@model IEnumerable<VM.DataAccessLayer.EDM.ProjectUserAssociateTable>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_CoreAdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CreatedTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RemovedTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Locked)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsOwner)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccountTable.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProjectTable.ProjectName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RemovedTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Locked)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsOwner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccountTable.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ProjectTable.ProjectName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
