@model IEnumerable<VM.DataAccessLayer.EDM.ModuleTable>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_CoreAdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ModuleName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModuleDLLname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Created)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Version)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeveloperName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StableVersion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UpdatedDLL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PathToDLL)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModuleDLLname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Created)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Version)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeveloperName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StableVersion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UpdatedDLL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PathToDLL)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
